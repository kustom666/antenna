.breadCrumb { padding: 0; float: left; display: block; height: 21px; overflow: hidden; width: 100%; padding: 5px;  }
.breadCrumb ul { margin: 0; padding: 0; height: 21px; display: block; }
.breadCrumb ul li { display: block; float: left; position: relative; height: 21px; overflow: hidden; line-height: 21px; margin: 0px 6px 0px 0; padding: 0px 14px 0px 4px; font-size: .9167em; background: url(<%= asset_path "Chevron.gif"%>) no-repeat 100%; }
