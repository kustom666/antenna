<h1>Listing index_pages</h1>

<table>
  <tr>
    <th>Title</th>
    <th>Body</th>
    <th></th>
    <th></th>
    <th></th>
  </tr>

<% @index_pages.each do |index_page| %>
  <tr>
    <td><%= index_page.title %></td>
    <td><%= index_page.body %></td>
    <td><%= link_to 'Show', index_page %></td>
    <td><%= link_to 'Edit', edit_index_page_path(index_page) %></td>
    <td><%= link_to 'Destroy', index_page, method: :delete, data: { confirm: 'Are you sure?' } %></td>
  </tr>
<% end %>
</table>

