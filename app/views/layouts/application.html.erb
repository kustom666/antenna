<!DOCTYPE html>
<html>
<head>
  <title>Antenna</title>
  <%= stylesheet_link_tag    "application", :media => "all" %>
  <%= javascript_include_tag "application" %>
  <%= csrf_meta_tags %>
</head>
<body>
	<% if current_user %>
		<%= link_to "Welcome #{current_user.nickname}", user_path(current_user) %>
		<%= link_to "logout", logout_path %>
	<%else%>
		<%= link_to "register", register_path %>
		<%= link_to "login", login_path %>
	<%end%>
<%= yield %>

</body>
</html>
